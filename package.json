{
  "name": "upm-react-ui",
  "version": "1.0.0",
  "description": "Componentes react UPM ",
  "keywords": [
    "frontend",
    "ui",
    "library",
    "react"
  ],
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "scripts": {
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "generate-props-doc": "react-docgen ./src/components ./src/providers --pretty --out example/src/props-doc/props.json",
    "test": "run-s test:unit test:lint test:build",
    "test:lint": "eslint src/**/*.js",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom --coverage --collectCoverageFrom=src/components/**/*.js",
    "test:build": "run-s build",
    "lint:fix": "prettier-eslint '**/*.js' --write"
  },
  "files": [
    "dist"
  ],
  "author": "alberto.nieto.peg@upm.es",
  "license": "MIT",
  "peerDependencies": {
    "prop-types": "^15.7.2",
    "react": ">=16.0.0"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "@uiw/react-md-editor": "^3.3.6",
    "clsx": "^1.1.1",
    "react-resize-observer": "^1.1.1",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.5",
    "@babel/plugin-proposal-export-default-from": "^7.14.5",
    "@babel/preset-env": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^13.1.9",
    "babel-eslint": "^10.1.0",
    "babel-loader": "8.1.0",
    "babel-plugin-macros": "^3.1.0",
    "concurrently": "^6.2.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-standard": "^5.0.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "2.3.1",
    "prettier-eslint": "^9.0.2",
    "prettier-eslint-cli": "^5.0.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-docgen": "^5.4.0",
    "react-dom": "^17.0.2",
    "react-scripts": "^4.0.3",
    "react-test-renderer": "^17.0.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.51.2",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-uglify": "^6.0.4",
    "rollup-plugin-url": "^3.0.1",
    "standard-react": "^4.0.0"
  }
}
